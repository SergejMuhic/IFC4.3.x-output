msgstr "Colocar en"

msgid "IfcObjectPlacement_PlacementRelTo_DEFINITION"
msgstr "Colocar en"

#:HarmonisedUMLmodel.xml:9493
msgid "IfcObjectPlacement_PlacesObject"
msgstr "Colocar objeto"

