#:HarmonisedUMLmodel.xml:21187
msgid "IfcMaterialProfileWithOffsets_OffsetValues"
msgstr "DeslocamentoValores"

msgid "IfcMaterialProfileWithOffsets_OffsetValues_DEFINITION"
